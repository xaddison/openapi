{
  "version": 2,
  "alias": ["open-api.zeit.sh"],
  "name": "open-api",
  "builds": [
    {
      "src": "site/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/bundle.js",
      "headers": { "cache-control": "s-maxage=0" },
      "dest": "/site/bundle.js"
    },
    {
      "src": "/swagger.yaml",
      "headers": { "cache-control": "s-maxage=0" },
      "dest": "/site/swagger.yaml"
    },
    {
      "src": "/favicon.ico",
      "headers": { "cache-control": "s-maxage=0" },
      "dest": "/site/favicon.ico"
    },
    {
      "src": "/(.*)",
      "headers": { "cache-control": "s-maxage=0" },
      "dest": "/site/index.html"
    }
  ]
}
