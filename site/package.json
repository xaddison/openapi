{
  "name": "@zeit/openapi-website",
  "description": "UI for the OpenApi Spec",
  "scripts": {
    "now-build": "yarn cp:assets && yarn cp:swagger && webpack",
    "cp:assets": "cpx 'src/{index.html,favicon.ico}' build/",
    "cp:swagger": "cpx '../swagger.yaml' build/",
    "clean": "rimraf build"
  },
  "keywords": [
    "openapi",
    "zeit"
  ],
  "author": "Javi Velasco <javi@zeit.co>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "14.14.37",
    "@types/react": "17.0.3",
    "@vercel/ncc": "0.27.0",
    "cpx": "1.5.0",
    "css-loader": "5.2.0",
    "postcss": "8.2.9",
    "postcss-import": "14.0.1",
    "postcss-loader": "5.2.0",
    "postcss-url": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "3.0.2",
    "style-loader": "2.0.0",
    "swagger-ui": "3.46.0",
    "ts-loader": "8.1.0",
    "typescript": "4.2.3",
    "webpack": "5.30.0",
    "webpack-cli": "4.6.0"
  }
}
